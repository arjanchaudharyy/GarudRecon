UNEWFLUSH() {
    START_SPINNER "${BGREEN}UNEWFLUSH${RESET}"
    # This is unew flush version only for gungnir saves output immediately in a file.
    git clone https://github.com/rix4uni/unew.git --depth 1 unewflush &>/dev/null
    cd unewflush
    mv unew.go unewflush.go
    sed -i 's/unew/unewflush/' go.mod
    sed -i '/w\.WriteString(line + "\\n")/a \\t\t\t\t\tw.Flush() // Flush immediately after writing' unewflush.go
    go install &>/dev/null
    cd ..
    rm -rf unewflush

