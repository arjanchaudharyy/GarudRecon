#!/bin/bash

# Default config file path
CONFIG_PATH="configuration/garudrecon.cfg"

# Help function
function show_help() {
  cat <<EOF
This command sets up GarudRecon by installing the required tools and dependencies for a specified reconnaissance function. It supports installation for various predefined scopes such as SMALLSCOPE, MEDIUMSCOPE, LARGESCOPE, and CRONJOBS, or you can install everything at once using ALL.

You can also provide a custom configuration file and enable verbose output for more detailed installation logs.

Usage:
  garudrecon install [flags]

Flags:
  -f, --function        Function to run (e.g. MEDIUMSCOPE)
  -c, --config          Custom configuration file path
  -v, --verbose         enable verbose mode
  -h, --help            help for install

Example:
  garudrecon install -f SMALLSCOPE
  garudrecon install -f MEDIUMSCOPE
  garudrecon install -f LARGESCOPE
  garudrecon install -f CRONJOBS
  garudrecon install -f ALL
EOF
}

# Check if no args
if [[ $# -eq 0 ]]; then
  show_help
  exit 0
fi

# Parse arguments
while [[ "$#" -gt 0 ]]; do
  case "$1" in
    -f|--function)
      if [[ -z "$2" || "$2" == -* ]]; then
        echo "Error: -f requires a valid function argument"
        exit 1
      fi

      FUNC="$2"
      shift 2
      ;;
    -c|--config)
      CONFIG_PATH="$2"
      shift 2
      ;;
    -v|--verbose)
      VERBOSE=true
      shift
      ;;
    -h|--help)
      show_help
      exit 0
      ;;
    *)
      echo "Unknown option: $1"
      exit 1
      ;;
  esac
done

# Load configuration
if [[ -f "$CONFIG_PATH" ]]; then
    source "$CONFIG_PATH"
else
    echo "Error: Configuration file '$CONFIG_PATH' not found."
    exit 1
fi

PRINTBANNER

# Check root user
AREUROOTUSER

# Creating directories
mkdir -p wordlists
mkdir -p takeovers

# Function to handle CLEANUP on Ctrl+C
CLEANUP() {
    if [[ -n "$SPINNER_PID" ]]; then
        kill "$SPINNER_PID" 2>/dev/null
    fi
    exit 1
}
trap CLEANUP SIGINT

START_SPINNER() {
    processing="${1}"
    START_TIME=$(date +%s)
    BRAILLE_STYLE_SPINNER_CHARS=("â ‹" "â ™" "â ¹" "â ¸" "â ¼" "â ´" "â ¦" "â §" "â ‡" "â ")

    while true; do
    	ELAPSED=$(( $(date +%s) - START_TIME ))
        for char in "${BRAILLE_STYLE_SPINNER_CHARS[@]}"; do
            printf "${YELLOW}[${char}]${RESET} ${processing} ${CYAN}(${ELAPSED} seconds)${RESET} ðŸ”Ž"
            printf "                                    \r"
            sleep 0.05
        done
    done &
    SPINNER_PID=$!
}

STOP_SPINNER() {
    kill "${SPINNER_PID}" 2>/dev/null
    TIME=$(( $(date +%s) - START_TIME ))
    return $TIME
}

echo -e "Using $CONFIG_PATH"


# VARIABLES
VARIABLES() {
    START_SPINNER "${BGREEN}VARIABLES${RESET}"
    echo -e "\n# xsel Variable" >> ~/.bashrc
    echo -e "alias xc='xsel -i -b'" >> ~/.bashrc
    echo -e "alias xp='xsel -o -b'" >> ~/.bashrc
    echo -e "alias cfile='explorer.exe .'" >> ~/.bashrc

    export PATH=$PATH:/usr/local/go/bin
    export PATH=$PATH:~/go/bin
    export PATH=$PATH:~/.local/bin
    export PATH=$PATH:~/.cargo/bin

    echo 'export PATH=$PATH:/usr/local/go/bin' >> ~/.bashrc
    echo 'export PATH=$PATH:~/go/bin' >> ~/.bashrc
    echo 'export PATH=$PATH:~/.local/bin' >> ~/.bashrc
    echo 'export PATH=$PATH:~/.cargo/bin' >> ~/.bashrc
    source ~/.bashrc

    STOP_SPINNER
    echo -e "${BGREEN}[+] VARIABLES - INSTALLED${RESET}: ${TIME} seconds"
}

# PACKAGES
PACKAGES() {
    START_SPINNER "${BGREEN}PACKAGES${RESET}"
    apt update -y &>/dev/null
    apt upgrade -y &>/dev/null

    # https://stackoverflow.com/questions/75602063/pip-install-r-requirements-txt-is-failing-this-environment-is-externally-mana
    rm -rf /usr/lib/python3.*/EXTERNALLY-MANAGED &>/dev/null

    STOP_SPINNER
    echo -e "${BGREEN}[+] PACKAGES - INSTALLED${RESET}: ${TIME} seconds"
}

PYTHON3() {
    START_SPINNER "${BGREEN}PYTHON3${RESET}"
    apt install python3 -y &>/dev/null

    if ! command -v python3 &>/dev/null; then
        echo -e "${BRED}[x] PYTHON3 - INSTALLATION FAILED${RESET}: ${TIME} seconds"
        STOP_SPINNER
        return
    fi

    STOP_SPINNER
    echo -e "${BGREEN}[+] PYTHON3 - INSTALLED${RESET}: ${TIME} seconds"
}

