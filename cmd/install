    gocl -i github.com/rix4uni/linkinspector &>/dev/null

    if ! command -v linkinspector &>/dev/null; then
        echo -e "${BRED}[x] LINKINSPECTOR - INSTALLATION FAILED${RESET}: ${TIME} seconds"
        STOP_SPINNER
        return
    fi

    STOP_SPINNER
    echo -e "${BGREEN}[+] LINKINSPECTOR - INSTALLED${RESET}: ${TIME} seconds"
}

