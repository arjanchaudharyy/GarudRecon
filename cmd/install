# CONFIGS
CONFIGS() {
    START_SPINNER "${BGREEN}CONFIGS${RESET}"
    mkdir -p ~/.config/favinfo && curl -s "https://raw.githubusercontent.com/rix4uni/favinfo/refs/heads/main/fingerprint.json" >> ~/.config/favinfo/fingerprint.json
    mkdir -p ~/.config/waymore && curl -s "https://raw.githubusercontent.com/xnl-h4ck3r/waymore/refs/heads/main/config.yml" >> ~/.config/waymore/config.yml
    mkdir -p ~/.config/xnLinkFinder && curl -s "https://raw.githubusercontent.com/xnl-h4ck3r/xnLinkFinder/refs/heads/main/config.yml" >> ~/.config/xnLinkFinder/config.yml
    mkdir -p ~/.config/gitrepoenum && curl -s "https://raw.githubusercontent.com/rix4uni/gitrepoenum/refs/heads/main/config/config.yaml" >> ~/.config/gitrepoenum/config.yml
    mkdir -p ~/.config/chaos && touch ~/.config/chaos/chaos-api.txt
    mkdir -p ~/.config/shosubgo && touch ~/.config/shosubgo/shosubgo-api.txt

    STOP_SPINNER
    echo -e "${BGREEN}[+] CONFIGS - INSTALLED${RESET}: ${TIME} seconds"
}


# SOME DATA
TRICKESTCLOUD() {
    START_SPINNER "${BGREEN}TRICKESTCLOUD${RESET}"
    wget -q -O wordlists/trickestcloud.zip https://github.com/trickest/cloud/archive/refs/heads/main.zip
    unzip -q wordlists/trickestcloud.zip -d wordlists
    mv wordlists/cloud-main wordlists/trickestcloud
    cat wordlists/trickestcloud/ssl/ssl*.csv | cut -d"," -f2,7 | tr "|" "\n" | tr "," "\n" | sed 's/"//g' | unew -el -i -t -q wordlists/trickestcloud_ssl.txt

    rm -rf wordlists/trickestcloud.zip wordlists/trickestcloud

    STOP_SPINNER
    echo -e "${BGREEN}[+] TRICKESTCLOUD - INSTALLED${RESET}: ${TIME} seconds"
}


# WORDLISTS
WORDLISTS() {
    START_SPINNER "${BGREEN}WORDLISTS${RESET}"
    curl -s "https://wordlists-cdn.assetnote.io/data/manual/best-dns-wordlist.txt" | unew -el -t -q wordlists/best-dns-wordlist.txt
    curl -s "https://wordlists-cdn.assetnote.io/data/manual/2m-subdomains.txt" | unew -el -t -q wordlists/2m-subdomains.txt
    curl -s "https://wordlists-cdn.assetnote.io/data/kiterunner/swagger-wordlist.txt" | unew -el -t -q wordlists/swagger-wordlist.txt
