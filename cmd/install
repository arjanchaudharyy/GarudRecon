    source ~/.bashrc

    if ! command -v rustup &>/dev/null; then
        echo -e "${BRED}[x] RUSTUP - INSTALLATION FAILED${RESET}: ${TIME} seconds"
        STOP_SPINNER
        return
    fi

    if ! command -v rustc &>/dev/null; then
        echo -e "${BRED}[x] RUSTC - INSTALLATION FAILED${RESET}: ${TIME} seconds"
        STOP_SPINNER
        return
    fi

    if ! command -v cargo &>/dev/null; then
